<view class="cardArea">
    <view class='cardA'>
        <view class='numA'>
            <view class='money'>
                <text>{{balance}}元</text>
                <text class='small'>代领总收入</text>
            </view>
            <view class='achie'>
                <text>{{creditScore}}</text>
                <text class='small'>LV.{{level}} 信用币</text>
            </view> 
        </view>

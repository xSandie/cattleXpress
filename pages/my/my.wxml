        <view hover-class="answerHover1" class='addCre'>
            <text>充值</text>
        </view>
        <view class='tline'>
        </view>
        <view hover-class="answerHover1" class='drawCash'>
            <text>提现</text>
        </view>
    </view> -->
    </view>
    <image class='cardBack' src='{{cardBg}}'></image>
</view>
        
    <view class='proA'>
        <view class='prof'>
            <open-data type="userAvatarUrl" class='profImg'></open-data>
        </view>
        <view class='info'>
            <view class='nameA'>
                <view wx:if="{{!haveCertif}}">
                    <open-data type="userNickName"></open-data>
                </view> 
                <view class='certif' wx:else>
                    <text>{{realName}}</text>
                    <view class='cerTag'>
                        <text>已认证</text>
                    </view>
                </view>
            </view>
            <view class='schoNum'>
                <view wx:if="{{!haveCertif}}">
                    <text>学号：66666666</text>
                </view>
                <view wx:else>
                    <text>学号：{{schoolNumb}}</text>
                </view>
            </view>
        </view>

    </view>
        <view class='bigBtn'>
            <view hover-class="answerHover1" class='address' bindtap="toAddrEdit">
                <view class='addressIcon'>
                    <image class='addressIcon' src="{{addressIcon}}"></image>
                </view>
                <text>收货地址管理</text>
            </view>
            <view class='cline'>
            </view>
            <view hover-class="answerHover1" class='police' bindtap="toPoList">
                <view class='policeIcon'>
                    <image class='policeIcon' src="{{policeIcon}}"></image>
                </view>
                <text>举报/申诉进度</text>
            </view>
        </view>
        <form report-submit="true" report-submit-timeout="1000" bindsubmit="collect" class='smallBtn'>
            <button form-type="submit" hover-class="answerHover1" class='joinUs defpadding' bindtap="myCode">
                <view class='joinUsText'>
                    <text>收款二维码</text>
                </view>
                <view class='next'>
                   <image class='nextIcon' src='{{nextIcon}}'></image> 
                </view>
            </button>
            <button form-type="submit" hover-class="answerHover1" class='joinUs defpadding' bindtap="toCertif">
                <view class='joinUsText'>
                    <text>更改认证信息</text>
                </view>
                <view class='next'>
                    <image class='nextIcon' src='{{nextIcon}}'></image>
                </view>
            </button>
            <!--<button form-type="submit" hover-class="answerHover1" class='joinUs defpadding' bindtap="doubleCertif">-->
                <!--<view class='supportText'>-->
                    <!--<text>学生证认证</text>-->
                <!--</view>-->
                <!--<view class='next'>-->
                    <!--<image class='nextIcon' src='{{nextIcon}}'></image>-->
                <!--</view>-->
            <!--</button>-->
            <button form-type="submit" open-type='contact' hover-class="answerHover1" class='joinUs defpadding'>
                <view class='joinUsText'>
                    <text>联系客服</text>
                </view>
                <view class='next'>
                    <image class='nextIcon' src='{{nextIcon}}'></image>
                </view>
            </button>
            <button form-type="submit" open-type='feedback' hover-class="answerHover1" class='joinUs defpadding'>
                <view class='joinUsText'>
                    <text>意见反馈</text>
                </view>
                <view class='next'>
                   <image class='nextIcon' src='{{nextIcon}}'></image> 
                </view>
            </button>
            <!-- <button form-type="submit" hover-class="answerHover1" class='joinUs defpadding' bindtap="agreement">
                <view class='joinUsText'>
                    <text>用户协议&隐私政策</text>
                </view>
                <view class='next'>
                   <image class='nextIcon' src='{{nextIcon}}'></image>
                </view>
            </button> -->
        </form>